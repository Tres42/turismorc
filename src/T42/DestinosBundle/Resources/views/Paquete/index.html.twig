<h1>Paquetes</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Titulo</th>
            <th>Fecha de Salida</th>
            <th>Es Grupal</th>
            <th>Es Promocion</th>
            <th>Observaciones</th>
            <th>Tarifas</th>
            <th>Servicio Incluido</th>
            <th>Servicio No Incluido</th>
            <th>Resumen</th>
            <th>Categoria</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('destinos_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.titulo }}</td>
            <td>{% if entity.fechaSalida %}{{ entity.fechaSalida|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.esGrupal }}</td>
            <td>{{ entity.esPromocion }}</td>
            <td>{{ entity.observaciones }}</td>
            <td>{{ entity.tarifas }}</td>
            <td>{{ entity.servicioIncluido }}</td>
            <td>{{ entity.servicioNoIncluido }}</td>
            <td>{{ entity.resumen }}</td>
            <td>{{ entity.categoria }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('destinos_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('destinos_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('destinos_new') }}">
            Crear nuevo paquete
        </a>
    </li>
</ul>
